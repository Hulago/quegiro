{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "quegiro",
  "private": true,
  "version": "0.0.1",
  "description": "Quegiro degiro csv transaction parser.",
  "author": {
    "name": "Hugo Lago",
    "email": "huzgo1@gmail.com"
  },
  "homepage": "https://github.com/Hulago/quegiro/",
  "repository": {
    "type": "git",
    "url": "git@github.com:Hulago/quegiro.git"
  },
  "engines": {
    "node": ">=16.12.0",
    "yarn": "^1.22.0"
  },
  "license": "MIT",
  "dependencies": {
    "@emotion/css": "^11.9.0",
    "@vueuse/core": "^9.4.0",
    "@vueuse/head": "^1.1.0",
    "ag-grid-community": "^27.3.0",
    "ag-grid-vue3": "^27.3.0",
    "date-fns": "^2.28.0",
    "date-fns-timezone": "^0.1.4",
    "echarts": "^5.3.2",
    "element-plus": "^2.2.26",
    "localforage": "^1.10.0",
    "lodash-es": "^4.17.21",
    "vue": "^3.2.36",
    "vue-i18n": "9.2.2",
    "vue-router": "^4.0.3",
    "vuedraggable": "^4.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.vue": "vue-tsc --noEmit",
    "*.{js,ts,json,yml,yaml,vue,htm,html}": "eslint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*.{css,sass,scss,vue,htm,html}": "stylelint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-stylelint",
    "*": "prettier -w -u"
  },
  "devDependencies": {
    "@iconify-json/ep": "^1.1.4",
    "@iconify-json/line-md": "^1.1.11",
    "@iconify-json/mdi": "^1.1.20",
    "@rushstack/eslint-patch": "^1.1.3",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.35",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "eslint": "^8.18.0",
    "eslint-plugin-vue": "^9.1.1",
    "sass": "^1.52.1",
    "typescript": "^4.7.2",
    "unocss": "^0.34.1",
    "unplugin-auto-import": "^0.8.8",
    "unplugin-icons": "^0.14.4",
    "unplugin-vue-components": "^0.19.5",
    "vite": "^2.9.9",
    "vite-plugin-purge-icons": "^0.8.1",
    "vite-ssg": "^0.20.0",
    "vite-svg-loader": "^3.4.0",
    "vue-tsc": "^0.34.16"
  },
  "scripts": {
    "build:analyze": "vite build --mode analyze",
    "build:clean": "rimraf dist",
    "build:dev": "vite build -c ./vite.config.ts --mode dev",
    "build": "vite build -c ./vite.config.ts --mode production",
    "clean": "rimraf ./node_modules/.vite",
    "lint": "eslint --ext .ts,vue --ignore-path .gitignore .",
    "preview": "vite preview",
    "serve": "vite serve -c ./vite.config.ts --mode local-dev --debug"
  }
}
