{
  "dependencies": {},
  "devDependencies": {
    "@quegiro/eslint-config": "^1.0.2",
    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "cross-env": "^5.2.0",
    "eslint": "^7.21.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^3.0.8",
    "lint-staged": "^10.0.8",
    "prettier": "^2.3.1",
    "rollup-plugin-web-worker-loader": "^1.6.1",
    "sass": "^1.49.7",
    "typescript": "4.5.2",
    "vite": "^2.7.2",
    "vite-plugin-vue2": "^1.9.0",
    "vite-svg-loader": "^3.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "MIT",
  "name": "prozis-sdk",
  "private": true,
  "scripts": {
    "build": "vite build -c ./vite.config.ts --mode production",
    "build:dashboard": "vite build ./apps/dashboard -c ./vite.config.ts --mode production --outDir ../../dist/dashboard",
    "build:irs": "vite build ./apps/organization -c ./vite.config.ts --mode production --outDir ../../dist/organization",
    "serve": "vite serve -c ./vite.config.ts --mode local-dev --debug",
    "serve:dashboard": "vite serve ./apps/dashboard -c ./vite.config.ts --mode local-dev --debug",
    "serve:irs": "vite serve ./apps/organization -c ./vite.config.ts --mode local-dev --debug"
  },
  "version": "1.0.0",
  "workspaces": [
    "packages/*",
    "apps/*",
    "libs/*",
    "containers/*"
  ]
}
